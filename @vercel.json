{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [{
    "src": "/bootstrap/(.*)",
            "dest": "/public/bootstrap/"
        },
        {
            "src": "/favicon/(.*)",
            "dest": "/public/favicon/"
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/css/"
        }, {
            "src": "/images/(.*)",
            "dest": "/public/images/"
        },
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    },

  ]
}
